page{
    background-color: #eee;
}

.integral-head{
    width: 750rpx;
    height: 240rpx;
    margin-bottom: 20rpx;
    background: url("http://static.memorecool.cn/wx/pet/integral-banner.png") no-repeat;
    background-size: 100% 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #ffb040;
    font-size: 28rpx;
}

.integral-logo{
    width: 40rpx;
    height: 44rpx;
}
.integral-head view{
    margin-top: 4rpx;
}
.integral-item{
    height: 86rpx;
    background-color: #fff;
    margin-bottom: 20rpx;
    display: flex;
    align-items: center;
    padding: 0 16rpx;
    font-size: 28rpx;
}
.integral-item image{
    width: 45rpx;
    height: 40rpx;
}

.integral-item-type{
    margin-left: 16rpx;
    flex: none;
}
.integral-item-time{
    flex: auto;
    text-align: center;
    color: #adadad;
    font-size: 12px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis
}
.integral-item-add{
    width: 60rpx;
    text-align: right;
}